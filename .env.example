# Frontend (.env.* files must use REACT_APP_ prefix)
REACT_APP_FIREBASE_API_KEY=
REACT_APP_FIREBASE_AUTH_DOMAIN=
REACT_APP_FIREBASE_PROJECT_ID=
REACT_APP_FIREBASE_STORAGE_BUCKET=
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=
REACT_APP_FIREBASE_APP_ID=
REACT_APP_FIREBASE_MEASUREMENT_ID=

# Optional: override Transak function URL and environment
# REACT_APP_CREATE_TRANSAK_SESSION_URL=https://<region>-<project>.cloudfunctions.net/createTransakSession
# REACT_APP_TRANSAK_ENV=STAGING

# Backend (set as Firebase Functions secrets; do NOT commit real values)
# firebase functions:secrets:set TRANSAK_API_KEY
# firebase functions:secrets:set TRANSAK_API_SECRET
# firebase functions:secrets:set TRANSAK_ENV           # STAGING or PRODUCTION
# firebase functions:secrets:set TRANSAK_ALLOWED_ORIGINS  # e.g. https://dev.web.app,https://prod.web.app
# firebase functions:secrets:set TRANSAK_SECRET       # optional legacy webhook HMAC secret
# firebase functions:secrets:set TRANSAK_REFERRER_DOMAIN # e.g. dev.web.app
